
CREATE TABLE INSTRUCTOR(
ins_id INT PRIMARY KEY NOT NULL,
lastname VARCHAR NOT NULL,
fristname VARCHAR NOT NULL,
city VARCHAR NOT NULL,
country CHAR(2)
-- );

